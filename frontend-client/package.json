{
  "name": "frontend-client",
  "version": "5.3.0",
  "description": "eSmiley Frontend Client",
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.esmiley.dk:7999/esr/frontend-client.git"
  },
  "author": "eSmiley A/S",
  "license": "MIT",
  "private": true,
  "keywords": [
    "react",
    "frontend",
    "foodwaste"
  ],
  "homepage": "https://www.esmiley.com",
  "engines": {
    "node": "12.18.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE > 10",
    "not dead"
  ],
  "nyc": {
    "include": [
      "src/*.js",
      "src/**/*.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "lcovonly"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "scripts": {
    "dev": "node --max-old-space-size=8192 node_modules/.bin/webpack serve --node-env=development --config webpack.dev.js --open",
    "profile-prod": "PROFILE=true webpack --node-env=production --config webpack.prod.js",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "test": "NODE_ICU_DATA=node_modules/full-icu NODE_OPTIONS='--trace-warnings' jest --maxWorkers=4",
    "test-integration": "NODE_ENV=test node_modules/.bin/wdio test/config/wdio/local.conf.js",
    "test-integration-browserstack": "NODE_ENV=test node_modules/.bin/wdio test/config/wdio/local.browserstack.conf.js",
    "start": "fe-server-start",
    "start:local": "NODE_ENV=test yarn start",
    "bundle": "fe-server-bundle",
    "boot": "npm install yarn -g && yarn install && node index.js",
    "build": "yarn install && node node_modules/.bin/webpack --node-env=production --config webpack.prod.js",
    "build:dev": "rm -rf dist && node node_modules/.bin/webpack --node-env=development --config webpack.dev.js",
    "build-docs": "gulp docs && styleguidist build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "retag-image": "SERVICE_NAME=frontend-server node_modules/.bin/retag-image",
    "build-and-push": "SERVICE_NAME=frontend-server NODE_BINARY='node_modules/.bin/fe-server-start' build-and-push",
    "upgrade-and-migrate": "RANCHER_SERVICE_NAME=app RANCHER_STACK_NAME=frontend RANCHER_SERVICE_PREFIX='/' REQUIRE_VALID_HASH=0 EXECUTE_MIGRATIONS=0 upgrade-and-migrate",
    "version": "printf 'Staging: %s\n' $(curl -s https://app.staging.esmiley.com/status | jq -r '.client.tag'); printf 'Production: %s\n' $(curl -s https://app.esmiley.com/status | jq -r '.client.tag')"
  },
